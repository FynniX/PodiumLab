{
  "name": "podiumlab",
  "version": "0.1.0",
  "description": "Useful Tools for ACC written in Node.js.",
  "author": "Fynn Haupt",
  "homepage": ".",
  "main": "public/electron.js",
  "dependencies": {
    "acc-node-wrapper": "^1.0.6",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "App/node_modules/.bin/react-scripts start",
    "build": "App/node_modules/.bin/react-scripts build",
    "test": "App/node_modules/.bin/react-scripts test",
    "eject": "App/node_modules/.bin/react-scripts eject",
    "dev": "App/node_modules/.bin/concurrently -k \\\"cross-env BROWSER=none npm start\\\" \\\"npm:electron\\\"",
    "electron": "App/node_modules/.bin/wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FynniX/PodiumLab.git"
  },
  "keywords": [
    "nodejs",
    "assetto-corsa-competizione"
  ],
  "bugs": {
    "url": "https://github.com/FynniX/PodiumLab/issues"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "electron": "^15.3.0",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^6.0.0"
  }
}
